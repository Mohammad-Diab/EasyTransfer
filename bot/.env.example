# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here

# Authorized Telegram User IDs (comma-separated)
# Example: 123456789,987654321
AUTHORIZED_USERS=

# Map Telegram User IDs to Account IDs for API calls
# Format: telegram_id1:account_id1,telegram_id2:account_id2
# Example: 123456789:1,987654321:2
USER_ACCOUNT_MAP=

# Server API URL
SERVER_URL=http://127.0.0.1:5000

# JWT Configuration
JWT_SECRET=MySuperStrongSecretKey!@#2025
JWT_EXPIRATION_DAYS=90
JWT_ALGORITHM=HS256